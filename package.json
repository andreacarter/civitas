  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mongoose": "^7.1.1",
    "react-scripts": "^5.0.1"
  },
  "engines": {
    "node": "8.11.3",
    "yarn": "1.9.4"
  }
}
